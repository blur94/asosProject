define('LazyLoaderBG',['jquery'], function($) {

    (function ($) {
        var options = $.lazyLoadXT,
            bgAttr = options.bgAttr || 'data-bg';

        options.selector += ',[' + bgAttr + ']';

        $(document).on('lazyshow', function (e) {
            var $this = $(e.target),
                $img = $('<img/>'),
                url;

            if ($this.attr(bgAttr)) {
                url = $this.attr(bgAttr);
            } else {
                return;
            }

            if(url === 'none') {
                $this
                .css('background-image', "none")
                .removeAttr(bgAttr)
                .triggerHandler('load');
            } else {
                $img.attr('src', url);
            }

            $img.on('load', function() {
                $this
                    .css('background-image', "url('" + url + "')")
                    .removeAttr(bgAttr)
                    .triggerHandler('load');
            });
        });

    })($);

});

