      return self.productIdColourList[productId] === item.colour;
    });

    if (!img) {
      img = imagesList.find(function (prod) {
        return prod.isPrimary;
      });
    }
    return (img && img.url) || (imagesList.length && imagesList[0].url) || "";
  };

