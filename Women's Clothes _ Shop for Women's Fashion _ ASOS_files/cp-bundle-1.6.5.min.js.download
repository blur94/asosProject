  Accordion.prototype.init = function (target) {
    const status = {
      visible: false,
      animating: false,
      panel: target.next("dd"),
      button: target.find("button"),
    };

    target.on("click", this.headerClick.bind(this, status));
  };

  Accordion.prototype.headerClick = function (status, event) {
    event.preventDefault();
    status.animating ||
      (status.visible ? this.hide(status) : this.show(status));
  };

  Accordion.prototype.show = function (status) {
    status.animating = true;
    status.button.addClass("open");
    status.button.attr("aria-expanded", "true");
    status.panel.attr("aria-hidden", "false");
    status.panel.slideDown(350, this.showComplete.bind(this, status));
  };

  Accordion.prototype.showComplete = function (status) {
    status.visible = true;
    status.animating = false;
  };

  Accordion.prototype.hide = function (status) {
    status.animating = true;
    status.panel.slideUp(350, this.hideComplete.bind(this, status));
  };

  Accordion.prototype.hideComplete = function (status) {
    status.visible = false;
    status.button.attr("aria-expanded", "false");
