        .parent()
        .find(nextButtonClass)
        .addClass("scrollableBanner-s" + index);
      container
        .find(swiperContainer)
        .parent()
        .find(prevButtonClass)
        .addClass("scrollableBanner-s" + index);
      container
        .find(swiperContainer)
        .parent()
        .find(paginationClass)
        .addClass("scrollableBanner-s" + index);

      const mySwiper = new Swiper(
        ".scrollablePromoBanner__swiper.scrollableBanner-s" + index,
        {
          loop: slidesCount > 1,
          watchOverflow: true,
