      const index = i + 1;
      const productCTARef = ctaRef + index;
      const productVariant = self.getImageVariant(product.id, product.images);

      product.outOfStockMessage = self.setOutOfStockMessage(
        self.browseCountry,
        self.outOfStockMessage
      );
      product.imageUrl = self.getImageUrl(product.id, product.images);

      if (
        product.isInStock &&
        product.price.current.value < product.price.previous.value
      ) {
        product.price.previous.displayText = product.price.previous.text;
      }

      if (
        product.isInStock &&
        product.price.current.value > product.price.previous.value &&
        product.price.current.value < product.price.rrp.value
      ) {
        product.price.previous.displayText = product.price.rrp.text;
      }

