define('CreateArticleFeed',["jquery", "ko"], function ($, ko) {
  let self;

  function CreateArticleFeed(opts) {
    self = this;
    self.defineObservables();
    self.el = $(opts.el);
    self.onErrorCallback = self.getOnErrorCallback(opts.onErrorCallback);
    self.locale = $("html").attr("data-language") || "en-GB";
    self.showCategory = self.getShowCategory();
    self.articleCount = self.getArticleCount();

    const dataUrl = self.getUrl();

    self.getData(dataUrl);
  }

  CreateArticleFeed.prototype.defineObservables = function () {
    self.articles = ko.observableArray([]);
  };

  CreateArticleFeed.prototype.getOnErrorCallback = function (callback) {
    if (typeof callback === "function") {
      return callback;
    }
    return function () {
      return "An error has occurred retrieving the Article Feed.";
    };
  };

  CreateArticleFeed.prototype.getUrl = function () {
    return self.el.attr("data-feed-url");
  };

  CreateArticleFeed.prototype.getShowCategory = function () {
    if (self.el.attr("data-show-category") === "1") {
      return true;
    }
    return false;
  };

  CreateArticleFeed.prototype.getArticleCount = function () {
    return parseInt(self.el.attr("data-articles-count"), 10);
  };

  CreateArticleFeed.prototype.getData = function (url) {
    $.ajax({
      url: url,
      method: "GET",
      dataType: "json",
    })
      .done(function (data, textStatus, jqXHR) {
        if (self.checkResponseCode(jqXHR.status)) {
          self.handleFeedVersion(data);
        } else {
          self.handleError();
        }
      })
      .fail(function () {
        self.handleError();
      });
  };

  CreateArticleFeed.prototype.handleError = function () {
    const storedArticles = self.getArticlesFromLocalStorage();

    if (storedArticles.length > 0) {
      self.handleData(storedArticles);
      return;
    }
    self.onErrorCallback();
  };

  CreateArticleFeed.prototype.checkResponseCode = function (responseCode) {
    if (responseCode === 200) {
      return true;
    }
    return false;
  };

  CreateArticleFeed.prototype.handleFeedVersion = function (data) {
    if (data.items) {
      self.checkIfFeedPopulated(data.items);
    } else {
      self.checkIfFeedPopulated(data);
    }
  };

  CreateArticleFeed.prototype.checkIfFeedPopulated = function (data) {
    if (data.length === 0) {
      self.handleError();
    } else {
      self.handleData(data);
    }
  };

  CreateArticleFeed.prototype.handleData = function (articles) {
    if (self.checkIfLocalStorageAvailable()) {
      self.setArticlesToLocalStorage(articles);
    }

    articles.forEach(function (article) {
      if (!article.FormatedDate) {
        article.FormatedDate = self.formatDate(article.Date);
      }
      if (!article.Franchise) {
        article.Franchise = article.Category;
      }
      article.showCategory = self.showCategory;
    });

    const slicedArticles = articles.slice(0, self.articleCount);

    self.articles(slicedArticles);
    $(self.el).trigger("articleFeedPopulated");
  };

  CreateArticleFeed.prototype.checkIfLocalStorageAvailable = function () {
    const storage = window["localStorage"];
    const x = "__storage_test__";

    try {
      storage.setItem(x, x);
      storage.removeItem(x);
      return true;
    } catch (e) {
      return false;
    }
  };

  CreateArticleFeed.prototype.setArticlesToLocalStorage = function (articles) {
    localStorage.setItem("articles", JSON.stringify(articles));
  };

  CreateArticleFeed.prototype.getArticlesFromLocalStorage = function () {
    const storedArticles = JSON.parse(localStorage.getItem("articles"));

    if (storedArticles) {
      return storedArticles;
    }
    return [];
  };

  CreateArticleFeed.prototype.formatDate = function (pubDate) {
    const slicedPubDate = self.slicePubDate(pubDate);
    const isoDate = self.formatIsoDate(slicedPubDate);

    const dateObject = new Date(isoDate);
    const localeDate = self.createLocaleDate(dateObject);

    return localeDate;
  };

  CreateArticleFeed.prototype.slicePubDate = function (date) {
    return date.slice(0, 8);
  };

  CreateArticleFeed.prototype.formatIsoDate = function (date) {
    const firstSeparation = [date.slice(0, 6), "/", date.slice(6)].join("");

    return [firstSeparation.slice(0, 4), "/", firstSeparation.slice(4)].join(
      ""
    );
  };

  CreateArticleFeed.prototype.createLocaleDate = function (dateObject) {
    const options = { day: "numeric", month: "long", year: "numeric" };
    return dateObject.toLocaleDateString(self.locale, options);
  };

  return CreateArticleFeed;
});

define('SetArticleHeight',["jquery"], function ($) {
  function SetArticleHeight(opts) {
    const articleFeedEl = $(opts.el);
    const articles = articleFeedEl.find(".articleFeedItem");

    const articleHeightArray = this.getArticleHeights(articles);
    const articleMaxHeight = this.getMaxHeight(articleHeightArray);

    this.onEvents(articleFeedEl);
    this.setArticleHeight(articles, articleMaxHeight);
  }

  //Event listener for window resize
  SetArticleHeight.prototype.onEvents = function (articles) {
    const self = this;

    $(window).resize(function () {
      const articleHeightArray = self.getArticleHeights(articles);
      const articleMaxHeight = self.getMaxHeight(articleHeightArray);
      self.setArticleHeight(articles, articleMaxHeight);
    });
  };

  SetArticleHeight.prototype.getArticleHeights = function (articles) {
    const array = [];

    for (let i = 0; i < articles.length; i++) {
      if (articles[i] === null) {
        return;
      }
      array.push($(articles[i]).find(".articleFeedItem__content").height());
    }

    return array;
  };

  SetArticleHeight.prototype.getMaxHeight = function (articles) {
    return Math.max.apply(Math, articles);
  };

  SetArticleHeight.prototype.setArticleHeight = function (
    articles,
    articleMaxHeight
  ) {
    $(articles).find(".articleFeedItem__content").height(articleMaxHeight);
  };

  return SetArticleHeight;
});

