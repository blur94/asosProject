define('AppendImage',["jquery"], function ($) {
  function appendImage(opts) {
    this.el = $(opts.el);
    const windowWidth = $(window).innerWidth();

    if (this.el.find("img").length === 0) {
      this.mobileImageUrl = this.el.data("image-mobile");
      this.desktopImageUrl = this.el.data("image-desktop");

      this.el.data("alt-mobile")
        ? (this.altMobile = this.el.data("alt-mobile"))
        : (this.altMobile = "");
      this.el.data("alt-desktop")
        ? (this.altDesktop = this.el.data("alt-desktop"))
        : (this.altDesktop = "");

      this.checkWindowWidth(windowWidth);
    }
  }

  appendImage.prototype.checkWindowWidth = function (windowWidth) {
    if (windowWidth < 736) {
      this.appendImage(this.mobileImageUrl, this.altMobile);
    }

    if (windowWidth >= 736) {
      this.appendImage(this.desktopImageUrl, this.altDesktop);
    }
  };

  appendImage.prototype.appendImage = function (imageUrl, alt) {
    this.el.append(
      '<img class="hero__image js-responsive-image" src="' +
        imageUrl +
