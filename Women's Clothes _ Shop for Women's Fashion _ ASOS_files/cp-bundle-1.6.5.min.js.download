define('ListOfLinks',["jquery"], function ($) {
  const ListOfLinks = function (target) {
    const windowWidth = $(window).innerWidth();
    this.uls = target;
    this.allListItems = this.uls.find("li");
    this.checkWindowWidth(windowWidth);
    this.onEvents();
  };

  ListOfLinks.prototype.onEvents = function () {
    const self = this;

    $(window).resize(function () {
      const windowWidth = $(window).innerWidth();
      self.checkWindowWidth(windowWidth);
    });
  };

  ListOfLinks.prototype.checkWindowWidth = function (windowWidth) {
    if (windowWidth >= 768) {
      this.resizeHandler();
    } else {
      this.setMobileHeight();
    }
  };

  ListOfLinks.prototype.resizeHandler = function () {
    const maxLength = this.findLongestList();
    this.resetHeights();
    this.resizeLiHeights(maxLength);
  };

  ListOfLinks.prototype.findLongestList = function () {
    const lengths = $.map(this.uls, function (ul) {
      return $(ul).find("li").length;
    });
    const maxLength = Math.max.apply(null, lengths);

    return maxLength;
  };

  ListOfLinks.prototype.resetHeights = function () {
    this.allListItems.height("auto");
  };

  ListOfLinks.prototype.resizeLiHeights = function (maxLength) {
    for (let i = 0; i < maxLength; i++) {
      const li = this.uls.find("li:eq(" + i + ")");
      let maxHeight = Math.max.apply(
        Math,
        $(li).map(function () {
          return $(this).height();
        })
      );
      $(li).height(maxHeight);
      maxHeight = -1;
    }
  };

  ListOfLinks.prototype.setMobileHeight = function () {
    this.allListItems.height(48);
  };

  return ListOfLinks;
});

