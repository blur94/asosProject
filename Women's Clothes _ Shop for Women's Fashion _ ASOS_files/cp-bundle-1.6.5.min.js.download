    ["mouseup", "touchend", "keydown"].forEach(function (eventName) {
      element.addEventListener(
        eventName,
        function (e) {
          switch (eventName) {
            case "keydown":
              if (e.key === "Enter" || spacebarKeyNames.includes(e.key)) {
