      this.player.canPlayType &&
      this.player.canPlayType("video/mp4") !== ""
    ) {
      this.setSrcAndPoster();
      this.setClassName();
      this.setPlaybackSettings();
      this.container.insertBefore(this.player, this.container.firstChild);

      if (this.autoplay) {
        this.onAutoEvents();
        this.toggleAutoplay();
      } else {
        this.player.preload = "metadata";
        this.provisionFullScreen();
        this.onClickEvents();
      }
    } else {
      const src = mediaQueryMatch(this.breakpoint)
        ? this.posterSecondary
        : this.posterPrimary;
      createHoldingImage(this.container, src, this.wrapper);
    }
  };

  /**
   * Send Adobe Analytics
   */
  VideoPlayer.prototype.sendAnalytics = function (
    videoState,
    playOption,
    pauseTime
  ) {
    const videoTitle =
      this.wrapper.querySelector(".videoClickToPlay-header__title") &&
      this.wrapper.querySelector(".videoClickToPlay-header__title").textContent;

    analyticsEventTrigger("tracking.home.pageInteraction", {
      global: {},
      context: videoState,
      video: {
        playState: "manual",
        playOption: playOption,
        playLength:
          Math.floor(this.player.duration) ||
          Number(
            this.player.timeStringDuration &&
              this.player.timeStringDuration.replace(":", "")
          ),
        pauseTime: pauseTime,
        videoTitle: this.player.title.length
          ? this.player.title
          : videoTitle
          ? videoTitle.trim()
          : "",
        videoUrl: this.player.src,
      },
    });

    if (playOption === "pause") {
      $(window).on("tracking.home.pageInteraction"),
        function (event) {
          event.stopPropagation();
        };
    }
  };

  VideoPlayer.prototype.stopAnalytics = function (event) {
    analyticsEventTrigger(function (event) {
      event.stopPropagation();
    });
  };

  /**
   * Handle events for the auto play context
   */
  /* istanbul ignore next */
  VideoPlayer.prototype.onAutoEvents = function () {
    const self = this;

    eventOnScroll(window, function () {
      self.toggleAutoplay();
    });

    document.addEventListener(
      "visibilitychange",
      function () {
        if (document.visibilityState === "hidden") {
          self.player.pause();
        } else {
          self.toggleAutoplay();
        }
      },
      false
    );
  };

  /**
   * Handle events for the click to play context
   */
  /* istanbul ignore next */
