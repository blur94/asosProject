      this.player.setAttribute("src", this.srcSecondary);
      this.player.setAttribute("poster", this.posterSecondary);
    } else {
      this.player.setAttribute("src", this.srcPrimary);
      this.player.setAttribute("poster", this.posterPrimary);
    }

